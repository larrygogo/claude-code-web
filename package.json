{
  "name": "claude-code-web",
  "version": "1.0.0",
  "private": true,
  "description": "Claude Code Web Service - Multi-user cloud deployment with Claude Agent SDK",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "turbo run clean && rimraf node_modules",
    "db:generate": "pnpm --filter server db:generate",
    "db:migrate": "pnpm --filter server db:migrate",
    "db:push": "pnpm --filter server db:push",
    "test": "node tests/run-all-tests.mjs",
    "test:api": "node tests/api-test.mjs",
    "test:e2e": "node tests/e2e-test.mjs",
    "test:browser": "node tests/e2e-browser-test.mjs"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "prettier": "^3.2.4",
    "puppeteer": "^24.36.1",
    "rimraf": "^5.0.5",
    "turbo": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.14.0",
  "engines": {
    "node": ">=18.0.0"
  }
}
